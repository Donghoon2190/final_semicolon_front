n\r\n  const editSubmit = async () => {\r\n    setLoading(true)\r\n    setCopycaption(editCaption.value);\r\n    await editPostMutation()\r\n    setLoading(false)\r\n    close()\r\n  }\r\n  const deletePost = async () => {\r\n    close()\r\n    setIsLoader(false)\r\n    await deletePostMutation();\r\n    setDeletePost(false)\r\n  }\r\n\r\n  return (<Popup\r\n    modal\r\n    overlayStyle={{ background: \"rgba(0,0,0,0.5\" }}\r\n    contentStyle={contentStyle}\r\n    closeOnDocumentClick={false}\r\n    trigger={(open) => (\r\n      <div className=\"menu\">\r\n        <Div>\r\n          <SpanS onClick={() => { deletePost() }}>삭제</SpanS>\r\n          <Span open={open}>제목수정</Span>\r\n          <Span onClick={close}>닫기</Span>\r\n        </Div>\r\n      </div >\r\n    )}\r\n  >\r\n\r\n    {(close) => (<Inputs>  <textarea placeholder={Copycaption} style={{ resize: \"none\" }} rows={7} cols={30} {...editCaption} onChange={editCaption.onChange} />\r\n      <Inputs><Button disabled={loading} onClick={() => { editSubmit() }}> {loading ? <Loader /> : <Text>수정</Text>} </Button>\r\n        <Button disabled={loading} onClick={close}> {loading ? <Loader /> : <Text>취소</Text>}</Button></Inputs></Inputs>)}\r\n  </Popup>)\r\n}\r\n","C:\\Users\\zmffh\\OneDrive\\Desktop\\TeamSemi\\semicolon_frontend\\src\\Components\\EventInfo\\index.js",[],"C:\\Users\\zmffh\\OneDrive\\Desktop\\TeamSemi\\semicolon_frontend\\src\\Components\\Button.js",[],"C:\\Users\\zmffh\\OneDrive\\Desktop\\TeamSemi\\semicolon_frontend\\src\\Components\\DetailPost\\index.js",[],"C:\\Users\\zmffh\\OneDrive\\Desktop\\TeamSemi\\semicolon_frontend\\src\\Components\\StateButton\\StateButtonQueries.js",[],"C:\\Users\\zmffh\\OneDrive\\Desktop\\TeamSemi\\semicolon_frontend\\src\\Components\\Menu.js",["214","215","216"],"import React, { useState } from \"react\";\r\nimport Popup from 'reactjs-popup';\r\nimport styled from \"styled-components\";\r\nimport { useMutation } from \"react-apollo-hooks\";\r\nimport { DELETE_COMMENT } fr